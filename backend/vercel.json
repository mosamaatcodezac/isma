{
  "version": 2,
  "buildCommand": "npm install --include=dev && npm run vercel-build",
  "installCommand": "npm install --include=dev",
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node",
      "config": {
        "includeFiles": [
          "dist/**",
          "node_modules/.prisma/**",
          "node_modules/@prisma/**"
        ]
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.js"
    }
  ],
  "outputDirectory": "."
}